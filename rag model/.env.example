# RAG API Environment Variables
# Copy this file to .env and fill in your actual values

# Groq AI API Key (required for answer generation)
# Get your API key from: https://console.groq.com/keys
GROQ_API_KEY=your_groq_api_key_here

# Groq Model to use (llama-3.3-70b-versatile is recommended for comprehensive answers)
GROQ_MODEL=llama-3.3-70b-versatile

# Port for RAG API server
RAG_API_PORT=5000

# Optional: Groq API endpoint (use default if not specified)
# GROQ_ENDPOINT=https://api.groq.com/v1
